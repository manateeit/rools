  "author": "",
  "license": "MIT",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^2.16.1",
    "@supabase/supabase-js": "^2.21.0",
    "axios": "^1.3.6",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inquirer": "^8.2.5",
    "next": "^13.3.1",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supabase": "^1.42.7"
  },
  "engines": {
    "node": ">=16.0.0"
