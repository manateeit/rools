{
  "name": "trading-ai-agent-bot",
  "version": "0.1.0",
  "description": "A trading bot that leverages LLM models to make trading decisions using the Alpaca API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "cli": "node scripts/start-cli.js",
    "web": "node src/index.js web",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare-db": "cat config/supabase-schema.sql | supabase db execute"
