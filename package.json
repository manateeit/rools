{
  "name": "trading-ai-agent-bot",
  "version": "0.1.0",
  "description": "A trading bot that leverages LLM models to make trading decisions using the Alpaca API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "cli": "node scripts/start-cli.js",
    "web": "node src/index.js web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern=__tests__/.*\\.test\\.js$",
    "test:integration": "jest --testPathPattern=__tests__/.*\\.integration\\.test\\.js$",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare-db": "cat config/supabase-schema.sql | supabase db execute",
    "build": "next build",
    "build:web": "cd web && next build",
    "start:web": "cd web && next start",
    "dev:web": "cd web && next dev"
  },
  "keywords": [
    "trading",
    "ai",
    "llm",
    "alpaca",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^2.16.1",
    "@supabase/auth-helpers-nextjs": "^0.6.0",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "axios": "^1.3.6",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inquirer": "^8.2.5",
    "next": "^13.3.1",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supabase": "^1.42.7"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "trading-ai-bot": "./scripts/start-cli.js"
  }
}