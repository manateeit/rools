{
  "version": 2,
  "name": "trading-ai-agent-bot",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/.next",
  "regions": ["iad1"],
  "github": {
    "silent": true,
    "autoAlias": true
  }
}